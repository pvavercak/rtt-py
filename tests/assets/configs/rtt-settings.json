{
    "toolkit-settings": {
        "logger": {
            "dir-prefix": "rtt-results/logs",
            "run-log-dir": "run-logs"
        },
        "result-storage": {
            "file": {
                "dir-prefix": "rtt-results/reports",
                "bsi-dir": "bsi",
                "fips-dir": "fips"
            }
        },
        "binaries": {
            "nist-sts": "/ws/rtt-statistical-batteries/nist-sts",
            "dieharder": "/ws/rtt-statistical-batteries/dieharder",
            "testu01": "/ws/rtt-statistical-batteries/testu01",
            "bsi": "/ws/rtt-statistical-batteries/bsi-src/build_ubuntu/bsi_battery",
            "fips": "/ws/rtt-statistical-batteries/fips-src/build_ubuntu/fips_battery"
        },
        "execution": {
            "max-parallel-tests": 8,
            "test-timeout-seconds": 3600
        }
    }
}
